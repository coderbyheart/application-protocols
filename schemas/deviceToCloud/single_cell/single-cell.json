{
    "title": "Single Cell Position",
    "description": "SCELL request",
    "type": "object",
    "properties": {
        "appId": {
            "type": "string",
            "const": "SCELL"
        },
        "messageType": {
            "type": "string",
            "const": "DATA"
        },
        "data": {
            "type": "object",
            "properties": {
                "doReply": {
                    "$ref": "#/definitions/DoReply"
                },
                "mcc": {
                    "$ref": "#/definitions/Mcc"
                },
                "mnc": {
                    "$ref": "#/definitions/Mnc"
                },
                "tac": {
                    "$ref": "#/definitions/Tac"
                },
                "eci": {
                    "$ref": "#/definitions/Eci"
                },
                "rsrp": {
                    "$ref": "#/definitions/Rsrp"
                },
                "format": {
                    "$ref": "#/definitions/Format"
                }
            },
            "required": [
                "mcc",
                "mnc",
                "tac",
                "eci"
            ],
            "additionalProperties": false
        }
    },
    "definitions": {
        "DoReply": {
            "type": "boolean",
            "description": "Does not reply, even in event of an error, if set to false. Defaults to true."
        },
        "Mcc": {
            "type": "integer",
            "description": "Mobile Country Code"
        },
        "Mnc": {
            "type": "integer",
            "description": "Mobile Network Code"
        },
        "Tac": {
            "type": "integer",
            "description": "Tracking Area Code"
        },
        "Eci": {
            "type": "integer",
            "description": "E-UTRA Cell Identifier"
        },
        "Rsrp": {
            "type": "number",
            "description": "Signal power",
            "minimum": -157,
            "maximum": -44
        },
        "Format": {
            "type": "string",
            "description": "Requested format. Defaults to binary.",
            "enum": [
                "json",
                "binary"
            ]
        }
    }
}