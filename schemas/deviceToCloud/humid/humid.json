{
    "title":"Humidity",
    "description":"The device's humidity sensor",
    "type":"object",
    "properties":{
       "appId":{
          "type":"string",
          "const":"HUMID"
       },
       "messageType":{
          "type":"string",
          "const":"DATA"
       },
       "data":{
          "type":"string",
          "description":"Relative humidity percentage",
          "pattern":"^(([0-9]|[1-9][0-9])\\.[0-9])|100.0$"
       },
       "ts": {
           "type": "number",
           "description": "Unix timestamp given in milliseconds when the data was sampled"
       }
    },
    "required":[
       "appId",
       "messageType",
       "data"
    ],
    "additionalProperties":false
 }
